<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Bootstrap demo</title>
    <link rel="stylesheet" href="../stylesheets/myv.css">
    <link href="https://cdn.jsdelivr.net/npm/remixicon@3.4.0/fonts/remixicon.css" rel="stylesheet">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@9/swiper-bundle.min.css" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-9ndCyUaIbzAi2FUVXJi0CjmCapSmO7SnpJef0486qhLnuZ2cdeRhO02iuK6FUUVM" crossorigin="anonymous">
</head>

<body>

    <div id="main">
        <a href="/myrented">
            <i id="close_page" class="ri-close-fill"></i>
        </a>
        <div class="swiper1 mySwiper1">
            <div class="swiper-wrapper">
                <% user.Bookmark.forEach(function(book){ %>
                    <% if(book.vehicleId._id.equals(myv._id)){ %>                       
                          <div class="swiper-slide">
                            
                                <div style="height: 90% !important;" id="div">
                                    <div class="left">
                                        <div class="swiper mySwiper">
                                            <div class="swiper-wrapper">
                                                <% book.vehicleId.pic.forEach(function(pic){ %>
                                                    <div class="swiper-slide">
                                                        <img src="../images/uploads/vehicleimages/<%= pic %>"
                                                            alt="">
                                                    </div>
                                                    <% }) %>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="right">
                                        <div class="user">
                                            <div class="uimg">
                                                <img src="../images/uploads/userimages/<%= user.userProfileimg %>"
                                                    alt="">
                                                <h6>
                                                    <%= user.name %>
                                                </h6>
                                            </div>
                                            <div class="cancel">
                                                <% const currentDate=new Date(); %>
                                                    <% if(currentDate>= book.startDate && currentDate <=
                                                            book.endDate){ %>
                                                            <small>
                                                                Journey Running
                                                            </small>
                                                            <% }else if(currentDate < book.startDate){
                                                                %>
                                                                <small>
                                                                    Cancel Booking
                                                                </small>
                                                                <% }else if(currentDate> book.endDate){
                                                                    %>
                                                                    <small>
                                                                        Jounrney completed
                                                                    </small>
                                                                    <% }else{ %>
                                                                        <small>
                                                                            Something Wrong
                                                                        </small>
                                                                        <% } %>

                                            </div>
                                        </div>
                                        <div class="botm">
                                            <div class="botm1">
                                                <h6>Vehicle Brand</h6>
                                                <h6>Vehicle Nama</h6>
                                                <h6>Vehicle Type</h6>
                                                <h6>Vehicle Colour</h6>
                                                <h6 class="m">Fuel Type</h6>
                                                <h6 class="m">Transmission Type</h6>
                                                <h6>Seating Capacity</h6>
                                                <h6>Air Conditioner </h6>
                                                <h6>Music System</h6>
                                                <h6 class="m">Year of Manufacture</h6>
                                                <h6>Rental Price (Per Day)</h6>
                                                <h6 class="m">Shop Name</h6>
                                                <h6 class="m">Shop Address</h6>
                                                <h6>Start Date</h6>
                                                <h6>End Date</h6>
                                                <h6 class="m">Ride Status</h6>
                                            </div>
                                            <div class="botm1">
                                                <h6>
                                                    <%= book.vehicleId.vehicleBrand %>
                                                </h6>
                                                <h6>
                                                    <%= book.vehicleId.vehicleName %>
                                                </h6>
                                                <h6>
                                                    <%= book.vehicleId.vehicleType %>
                                                </h6>
                                                <h6>
                                                    <%= book.vehicleId.vehicleColor %>
                                                </h6>
                                                <h6 class="m">
                                                    <%= book.vehicleId.fuelType %>
                                                </h6>
                                                <h6 class="m">
                                                    <%= book.vehicleId.transmissionType %>
                                                </h6>
                                                <h6 style="white-space: nowrap;">Maximum <%= book.vehicleId.seating %> Person Can Sit
                                                </h6 classm>
                                                <% if(book.vehicleId.Ac===true){ %>
                                                    <h6>Is Available</h6>
                                                    <% }else{ %>
                                                        <h6>Not Available</h6>
                                                        <% } %>
                                                            <% if(book.vehicleId.Es===true){ %>
                                                                <h6>Is Available</h6>
                                                                <% }else{ %>
                                                                    <h6>Not Available</h6>
                                                                    <% } %>
                                                                        <h6 class="m">
                                                                            <%= book.vehicleId.YOM.toLocaleString().split(',')[0]
                                                                                %>
                                                                        </h6>
                                                                        <h6>â‚¹ <%=
                                                                                book.vehicleId.RentalPrice
                                                                                %>
                                                                        </h6>
                                                                        <h6 class="m">
                                                                            <%= book.vehicleId.Shopname
                                                                                %>
                                                                        </h6>
                                                                        <h6 class="m">
                                                                            <%= book.vehicleId.Shopaddress
                                                                                %>
                                                                        </h6>
                                                                        <h6>
                                                                            <%= book.startDate.toLocaleString()
                                                                                %>
                                                                        </h6>
                                                                        <h6>
                                                                            <%= book.endDate.toLocaleString()
                                                                                %>
                                                                        </h6>

                                                                        <% if(currentDate>=
                                                                            book.startDate &&
                                                                            currentDate <=
                                                                                book.endDate){ %>
                                                                                <h6 class="m"
                                                                                    style="color: green; font-weight: 700;">
                                                                                    Jounrney is running
                                                                                </h6>
                                                                                <% }else if(currentDate
                                                                                    < book.startDate){
                                                                                    %>
                                                                                    <h6 class="m"
                                                                                        style="color: green; font-weight: 700;">
                                                                                        Journey is
                                                                                        comming</h6>
                                                                                    <% }else
                                                                                        if(currentDate>
                                                                                        book.endDate){
                                                                                        %>
                                                                                        <h6 class="m"
                                                                                            style="color: green; font-weight: 700;">
                                                                                            Jounrney is
                                                                                            completed
                                                                                        </h6>
                                                                                        <% }else{ %>
                                                                                            <h6 class="m"
                                                                                                style="color: green; font-weight: 700;">
                                                                                                Something
                                                                                                Wrong
                                                                                            </h6>
                                                                                            <% } %>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                          </div>
                    <% } %>
                <% }) %>
            </div>
            <div class="swiper-button-next"></div>
            <div class="swiper-button-prev"></div>
            <div class="swiper-pagination"></div>
          </div>
        </div>

    <script src="https://cdn.jsdelivr.net/npm/swiper@9/swiper-bundle.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-geWF76RCwLtnZ8qwWowPQNguL3RmwHVBC9FhGdlKrxdiJJigb/j/68SIy3Te4Bkz"
        crossorigin="anonymous"></script>
    <script>
        var swiper = new Swiper(".mySwiper", { autoplay: true, grabCursor: true, loop: true });

        var swiper1 = new Swiper(".mySwiper1", {
            pagination: {
                el: ".swiper-pagination",
                type: "progressbar",
            },
            loop:true,
            navigation: {
                nextEl: ".swiper-button-next",
                prevEl: ".swiper-button-prev",
            },
        });
    </script>
</body>

</html>