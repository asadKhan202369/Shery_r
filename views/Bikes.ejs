              <script>
                const scroller = new LocomotiveScroll({
                  el: document.querySelector("#main"),
                  smooth: true
                });
                scroller.update()
  
              
                // document.querySelector("#menu-icon").addEventListener("click", function() {
                //   document.querySelector("#fullscreen-nav").style.left = "15vw"
                // })
                document.querySelector("#cross").addEventListener("click", function() {
                  document.querySelector("#fullscreen-nav").style.left = "100vw"
                })
  
              
                document.addEventListener("DOMContentLoaded", function(event) {
                  var scrollpos2 = localStorage.getItem('scrollpos2');
                  if (scrollpos2.split('px')[0] > 10) {
                    // scroller.scrollTo(0, scrollpos2);
                  }
                  setTimeout(function() {
                    scroller.update();
                  }, 500)
                });
  
                scroller.on("scroll", function(dets) {
                  localStorage.setItem('scrollpos2', dets.scroll.y + "px");
                })
                var flags = 0;
                var tempscrollpos = 0;
                scroller.on("scroll", function(dets) {
                  var scrollpos2 = localStorage.getItem('scrollpos2');
                  if (scrollpos2.split('.')[0] > 50) {
                    flags = 1;
                  }
                  if (scrollpos2.split('.')[0] < 50 || Number(scrollpos2.split('p')[0]) < Number(tempscrollpos)) {
                    tempscrollpos = Number(scrollpos2.split('p')[0])
                    flags = 0;
                  }
                  if (flags === 1) {
                    document.querySelector('#nav-sec-main').style.opacity = "0";
                    document.querySelector('#nav-sec-main').style.transform = 'translateY(-100px)';
                  }
                  if (flags === 0) {
                    document.querySelector('#nav-sec-main').style.opacity = "1";
                    document.querySelector('#nav-sec-main').style.transform = 'translateY(0px)';
                  }
  
                  if (Number(scrollpos2.split('p')[0]) > Number(tempscrollpos)) {
                    tempscrollpos = Number(scrollpos2.split('p')[0])
                  }
  
                  var rang = ["#024FA9", "#BD2200", "#ED203D", "#B32DE2", "#D9D9D9", "#FF9900", "#12B5E5", "#34A47C", "#ff7023", "#F38BA3"]
                  document.querySelectorAll("#filter-tags h3").forEach(function(elem) {
                    elem.addEventListener("mouseenter", function() {
                      var randomrang = Math.floor(Math.random() * 10)
                      console.log(rang[randomrang])
                      console.log(randomrang)
                      elem.style.boxShadow = `2px 2px 0 ${rang[randomrang]}`
                    })
                    elem.addEventListener("mouseleave", function() {
  
                      elem.style.boxShadow = `0px 0px 0 #000`
                    })
                  })
                  })
                  
              </script>
