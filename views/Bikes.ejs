      #fullscreen-nav {
          height: 100vh;
          width: 84vw;
          display: none;
          /* justify-content: center; */
          /* align-items: center; */
          /* padding: 10px 40px; */
          flex-direction: column;
          background-color: #eeeeee;
          z-index: 9999999 !important;
          position: absolute;
          transition: all cubic-bezier(0.19, 1, 0.22, 1) 0.6s;
          left: 102vw;
          position: fixed;
          top: 0;
          padding-bottom: 30vw;
          /* background-color: red; */
      }
  
      #fullscreen-nav h4 {
          font-size: 2.5vw;
          font-weight: 900;
          padding: 4.5vh 36px;
          cursor: pointer;
          border-bottom: 1px solid #8f9191;
          color: #000;
          overflow: hidden;
      }
  
  
      #fullscreen-nav h5 {
          font-size: 2.5vw;
          position: absolute;
          cursor: pointer;
          top: 3vh;
          right: 6vw;
      }
  
      #fullscreen-nav h5 i {
          font-weight: 100;
          background-color: white;
          border-radius: 50%;
      }
  
      #fullscreen-nav h4 a {
          text-decoration: none;
          color: #222222;
          font-weight: 900;
          transition: all cubic-bezier(0.19, 1, 0.22, 1) 1s;
          text-rendering: optimizeLegibility;
          font-synthesis: none !important;
          -webkit-text-stroke: 0.6px;
  
      }
  
      #fullscreen-nav h4 a:hover {
          color: #000;
          font-weight: 500;
      }
  
      #changing-greeting-text {
          display: inline-block;
          overflow: hidden;
          height: 2.8vh;
          margin-bottom: -0.4vw;
          width: 2.8vw;
      }
  
      #greet-text {
          text-align: right;
          display: inline-block;
          transform: translateY(0vh);
          width: 2.6vw;
          animation: slideanimation 5s infinite alternate;
          animation-delay: 1s;
      }
  
      #greeting_user {
          height: 1.5vw;
          /*margin-top: 0.2vw;*/
          /* background-color: rebeccapurple; */
          overflow: hidden;
      }
  
      #popup_box {
          width: 17vw;
          height: 11vw;
          background-color: #e9f5ff6c;
          backdrop-filter: blur(15px);
          position: absolute;
          border-radius: 10px;
          z-index: 99999;
          top: 5.5vh; 
          right: 3vw !important;
          display: flex;
          justify-content: space-between;
          align-items: center;
          flex-direction: column;
          padding: 2vh 0;
          transition: all cubic-bezier(0.19, 1, 0.22, 1) 1s;
          display: none;
      }
      
      #popup_box h4 {
          font-weight: 100;
          font-size: 1.2vw;
          font-family: paytone one;
      }
  
      #popup_box a {
          padding: 1.1vh 2.5vw;
          border-radius: 3px;
          border: none;
          background-color: #12B5E5;
          cursor: pointer;
          box-shadow: 3px 3px 0px #00000070;
          transition: all cubic-bezier(0.19, 1, 0.22, 1) 0.3s;
      }
  
      #popup_box a {
          font-size: 1.1vw;
          color: #000;
          font-family: Paytone One;
          font-weight: 100;
          text-decoration: none;
      }
  
      #popup_box a:hover {
          box-shadow: 5px 5px 0px #000000ef;
          color: white;
      }
      #nav{
        background-color: #000 !important;
      }
      #popup_box a:active {
          transform: scale(0.95);
      }
  
      @media(max-width: 600px) {
  
          #nav-main {
              background-color: white;
              padding: 0 2vw 0 1vw;
              width: 100vw;
              height: 15vw;
              display: flex;
              align-items: center;
              justify-content: space-between;
              overflow-x: hidden;
  
          }
  
          #left-main-nav {
              width: 60vw;
              /* background-color: blue; */
              height: 100%;
              display: flex;
              justify-content: center;
              padding: 0 1vw 0 0;
              align-items: center;
          }
  
          #left-main-nav h4 {
              font-size: 3.5vw;
              text-rendering: optimizeLegibility;
          font-synthesis: none !important;
          -webkit-text-stroke: 0.5px
          }
  
          #left-main-nav img {
              height: 6.5vw;
          }
  
          #right-main-nav {
              transition: all cubic-bezier(0.19, 1, 0.22, 1) 1s;
              width: 30vw;
              height: 100%;
              /* background-color: yellow; */
              display: flex;
              justify-content: center !important;
              align-items: center;
              padding: 0 1.2vw;
              margin-right: -5vw;
          }
  
          #right-main-nav a {
              display: none;
              white-space: nowrap;
              margin-left: -30px;
          }
  
          #right-main-nav a h4 {
              font-weight: 900 !important;
          }
  
          #left-main-nav>a {
              /* font-size: 11px !important; */
              font-size: 3vw !important;
              transition: all cubic-bezier(0.19, 1, 0.22, 1) 1s;
              font-weight: 600;
              margin-left: 0px;
              margin-top: 0.1vh;
          }
          #left-main-nav>a h4{
              font-weight: 500;
          }
          #nav-main-line {
              width: 2px;
              margin: 0 2vw 0 2vw;
              height: 35%;
              background-color: rgb(157, 157, 157);
          }
  
          #right-main-nav>i {
              margin-left: 25px;
              font-size: 22px;
              color: #2181ff;
              cursor: pointer;
              font-weight: 700;
              display: initial;
              text-rendering: optimizeLegibility;
              font-synthesis: none !important;
              -webkit-text-stroke: 0.5px
          }
          #fullscreen-nav{
              display: flex;
          }
          #fullscreen-nav h5 {
              font-size: 9vw;
              height: 10vw;
              width: 10vw;
              font-weight: 500;
              position: relative;
              cursor: pointer;
              border-radius: 50%;
              background-color: white;
              right: 5vw;
  
          }
  
          #fullscreen-nav h4 i {
              position: relative;
              /* height: 20vw;
              width: 20vw; */
              left: 90%;
              top:0vh;
              font-weight: 100;
              font-size: 9vw;
              background-color: white;
              color: #000;
              padding: 0.8vw;
              border-radius: 50%;
          }
  
          #fullscreen-nav h4 a {
              font-size: 6vw;
              margin: 2vw 0;
              font-weight: 400;
              color: #000;
          }
      }
  
      @keyframes slideanimation {
          0% {
              transform: translateY(0);
          }
  
          5% {
              transform: translateY(0);
          }
  
          25% {
              transform: translateY(-1.3vw);
          }
  
          50% {
              transform: translateY(-2.6vw);
          }
  
          75% {
              transform: translateY(-3.9vw);
          }
  
          100% {
              transform: translateY(-5.2vw);
          }
      }
  </style>

